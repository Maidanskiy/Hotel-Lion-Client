{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "types": [
      "reflect-metadata"
    ],
    "noImplicitAny": false,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "exclude":[
    "node_modules",
    "./src/**/*.spec.ts"
  ]
}
